{
  "name": "backend-nodejs-challenge",
  "version": "1.0.0",
  "description": "This challenge aims to evaluate basic skills in Node.js development, and a bit of data/entity modeling. The idea is to build an HTTP REST API.",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "swagger-autogen": "node ./src/swagger.js",
    "db:migrate": "npx sequelize db:migrate",
    "db:seeds": "npx sequelize-cli db:seed:all",
    "reset-sqlite-dev": "npx rimraf *.sqlite && npm run db:migrate && npm run db:seeds",
    "test": "mocha"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.1",
    "sequelize-auto": "^0.8.8",
    "sequelize-cli": "^6.6.1",
    "swagger-autogen": "^2.23.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.0",
    "sqlite3": "^5.1.6",
    "winston": "^3.9.0"
  }
}
